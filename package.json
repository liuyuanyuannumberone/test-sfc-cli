{
  "name": "@charlesliu/test-sfc-cli",
  "version": "1.0.8",
  "description": "test-sfc-cli for Vue",
  "license": "MIT",
  "author": {
    "name": "lyy",
    "email": "lyy@foxmail.com",
    "url": "https://github.com/liuyuanyuannumberone/test-sfc-cli"
  },
  "bugs": {
    "url": "https://github.com/liuyuanyuannumberone/test-sfc-cli/issues"
  },
  "homepage": "https://github.com/liuyuanyuannumberone/test-sfc-cli#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/liuyuanyuannumberone/test-sfc-cli.git"
  },
  "keywords": [
    "vue",
    "sfc",
    "component"
  ],
  "files": [
    "src",
    "dist"
  ],
  "main": "dist/test-sfc-cli.umd.js",
  "module": "dist/test-sfc-cli.esm.js",
  "unpkg": "dist/test-sfc-cli.min.js",
  "jsnext:main": "dist/test-sfc-cli.min.js",
  "browser": {
    "./sfc": "src/test-sfc-cli.vue"
  },
  "types": "src/test-sfc-cli.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "http://192.168.48.160:8073/repository/npm-hosted-private/"
  },
  "scripts": {
    "dev": "vue-styleguidist server",
    "test": "jest --verbose",
    "doc": "vue-styleguidist build",
    "build": "npm run build:unpkg & npm run build:es & npm run build:umd & npm run doc",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/test-sfc-cli.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/test-sfc-cli.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/test-sfc-cli.min.js",
    "stdver": "standard-version -m '[skip ci] chore(release): v%s'",
    "release-patch": "standard-version --release-as patch",
    "release-minor": "standard-version --release-as minor",
    "release-major": "standard-version --release-as major",
    "release-static": "standard-version --release-as 3.3.3",
    "release": "gren release --override",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.4.3",
    "@femessage/github-release-notes": "latest",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.5",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "husky": "1.3.1",
    "jest": "^24.8.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.0",
    "minimist": "^1.2.0",
    "prettier": "1.18.2",
    "rollup": "^2.41.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-vue": "^4.7.2",
    "standard-version": "9.5.0",
    "stylelint": "^9.10.0",
    "stylelint-config-standard": "^18.2.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-styleguidist": "^3.16.3",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.29.6"
  },
  "vue-sfc-cli": "1.14.0",
  "engines": {
    "node": ">= 8.3.0",
    "npm": ">= 3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.@(md|json)": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  },
  "gren": "@femessage/grenrc"
}
